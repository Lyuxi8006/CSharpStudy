<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Bread\french_bread.png" />
    <None Remove="Assets\Bread\italian_bread.png" />
    <None Remove="Assets\Cheese\american.png" />
    <None Remove="Assets\Cheese\american_cheese.png" />
    <None Remove="Assets\Cheese\swiss.png" />
